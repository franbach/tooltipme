{
  "name": "tooltip-me",
  "description": "A simple and easy Vue 3 tooltip",
  "version": "2.0.1",
  "private": false,
  "author": {
    "name": "Francisco Bach",
    "email": "franciscobach@gmail.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name tooltip-me ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "repository": "https://github.com/franbach/tooltipme",
